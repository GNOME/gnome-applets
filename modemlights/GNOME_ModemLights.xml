<Root>
  <commands>
    <cmd name="Activate"   pixtype="stock" pixname="gtk-execute" sensitive="0"/>
    <cmd name="Deactivate" pixtype="stock" pixname="gtk-stop"    sensitive="0"/>
    <cmd name="Properties" pixtype="stock" pixname="gtk-properties"/>
    <cmd name="Help"       pixtype="stock" pixname="gtk-help"/>
    <cmd name="About"      pixtype="stock" pixname="gtk-about"/>
  </commands>

  <popups>
    <popup name="button3">
	 <menuitem name="Activate"   verb="" _label="_Activate"/>
	 <menuitem name="Deactivate" verb="" _label="_Deactivate"/>
	 <separator/>
	 <menuitem name="Properties" verb="" _label="_Properties"/>
	 <menuitem name="Help"       verb="" _label="_Help"/>
	 <menuitem name="About"      verb="" _label="_About"/>
    </popup>
  </popups>
</Root>
