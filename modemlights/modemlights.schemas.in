<gconfschemafile>
    <schemalist>
    
      <schema>
        <key>/schemas/apps/modemlights/prefs/delay</key>
        <owner>modemlights_applet2</owner>
        <type>int</type>
        <default>5</default>
        <locale name="C">
          <short>Update interval</short>
          <long>The fraction of a second until the applet updates.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/modemlights/prefs/lockfile</key>
        <owner>modemlights_applet2</owner>
        <type>string</type>
        <default></default>
        <locale name="C">
          <short>Modem lock file</short>
          <long>The name of the modem lock file.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/modemlights/prefs/connect</key>
        <owner>modemlights_applet2</owner>
        <type>string</type>
        <default>pppon</default>
        <locale name="C">
          <short>Command executed when connecting</short>
          <long>Use this command to connect the modem.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/modemlights/prefs/disconnect</key>
        <owner>modemlights_applet2</owner>
        <type>string</type>
        <default>pppoff</default>
        <locale name="C">
          <short>Command executed when disconnecting</short>
          <long>Use this command to disconnect the modem.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/modemlights/prefs/confirmation</key>
        <owner>modemlights_applet2</owner>
        <type>int</type>
        <default>1</default>
        <locale name="C">
          <short>Ask for confirmation when connecting/disconnecting</short>
          <long>Display a confirmation dialog when connecting or disconnecting.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/modemlights/prefs/device</key>
        <owner>modemlights_applet2</owner>
        <type>string</type>
        <default>ppp0</default>
        <locale name="C">
          <short>Modem device name</short>
          <long>The name of the modem device.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/modemlights/prefs/isdn</key>
        <owner>modemlights_applet2</owner>
        <type>int</type>
        <default>0</default>
        <locale name="C">
          <short>Use isdn</short>
          <long>Use isdn instead of ppp to connect the modem.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/modemlights/prefs/verify_lock</key>
        <owner>modemlights_applet2</owner>
        <type>int</type>
        <default>1</default>
        <locale name="C">
          <short>Verify owner of lock file</short>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/modemlights/prefs/extra_info</key>
        <owner>modemlights_applet2</owner>
        <type>int</type>
        <default>1</default>
        <locale name="C">
          <short>Show connect time and throughput</short>
          <long>Show extra information about the connect time and amount of data transmitted and received.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/modemlights/prefs/wait_blink</key>
        <owner>modemlights_applet2</owner>
        <type>int</type>
        <default>0</default>
        <locale name="C">
          <short>Blink when connecting</short>
          <long>Make the applet blink when the modem is connecting.</long>
        </locale>
      </schema> 
      
      <schema>
        <key>/schemas/apps/modemlights/prefs/color_rx</key>
        <owner>modemlights_applet2</owner>
        <type>string</type>
        <default>#FF0000</default>
        <locale name="C">
          <short>Receive foreground color</short>
          <long>The color used to indicate that data has been received.</long>
        </locale>
      </schema>
      <schema>
        <key>/schemas/apps/modemlights/prefs/color_rx_bg</key>
        <owner>modemlights_applet2</owner>
        <type>string</type>
        <default>#4D0000</default>
        <locale name="C">
          <short>Receive background color</short>
          <long>The background color of the button used to inidcate data received.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/modemlights/prefs/color_tx</key>
        <owner>modemlights_applet2</owner>
        <type>string</type>
        <default>#00FF00</default>
        <locale name="C">
          <short>Send foreground color</short>
          <long>The color used to indicate that data has been sent.</long>
        </locale>
      </schema>
      <schema>
        <key>/schemas/apps/modemlights/prefs/color_tx_bg</key>
        <owner>modemlights_applet2</owner>
        <type>string</type>
        <default>#004D00</default>
        <locale name="C">
          <short>Send background color</short>
          <long>The background color of the button used to inidcate data sent.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/modemlights/prefs/color_status_bg</key>
        <owner>modemlights_applet2</owner>
        <type>string</type>
        <default>#004D00</default>
        <locale name="C">
          <short>Status not connected color</short>
          <long>The color used to display the status button when the modem is not connected.</long>
        </locale>
      </schema>
      <schema>
        <key>/schemas/apps/modemlights/prefs/color_status_ok</key>
        <owner>modemlights_applet2</owner>
        <type>string</type>
        <default>#00FF00</default>
        <locale name="C">
          <short>Status connected color</short>
          <long>The color used to display the status button when the modem is connected.</long>
        </locale>
      </schema>
      <schema>
        <key>/schemas/apps/modemlights/prefs/color_status_wait</key>
        <owner>modemlights_applet2</owner>
        <type>string</type>
        <default>#FEF06B</default>
        <locale name="C">
          <short>Status waiting connection color</short>
          <long>The color used to display the status button when the modem is connecting.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/modemlights/prefs/color_text_bg</key>
        <owner>modemlights_applet2</owner>
        <type>string</type>
        <default>#0000000</default>
        <locale name="C">
          <short>Text background color</short>
          <long></long>
        </locale>
      </schema>
      <schema>
        <key>/schemas/apps/modemlights/prefs/color_text_fg</key>
        <owner>modemlights_applet2</owner>
        <type>string</type>
        <default>#00FF00</default>
        <locale name="C">
          <short>Text foreground color</short>
        </locale>
      </schema>
      <schema>
        <key>/schemas/apps/modemlights/prefs/color_text_mid</key>
        <owner>modemlights_applet2</owner>
        <type>string</type>
        <default>#004D00</default>
        <locale name="C">
          <short>Text outline color</short>
        </locale>
      </schema>
        
    </schemalist>

</gconfschemafile>  
