<gconfschemafile>
<schemalist>

<schema>
	<key>/schemas/apps/stickynotes_applet/defaults/width</key>
	<applyto>/apps/stickynotes_applet/defaults/width</applyto>
	<owner>stickynotes_applet</owner>
	<type>int</type>
	<default>100</default>
	<locale name="C">
		<short>Default width for new notes</short>
		<long>Default width for new notes in pixels.</long>
	</locale>
</schema>

<schema>
	<key>/schemas/apps/stickynotes_applet/defaults/height</key>
	<applyto>/apps/stickynotes_applet/defaults/height</applyto>
	<owner>stickynotes_applet</owner>
	<type>int</type>
	<default>100</default>
	<locale name="C">
		<short>Default height for new notes</short>
		<long>Default height for new notes in pixels.</long>
	</locale>
</schema>

<schema>
	<key>/schemas/apps/stickynotes_applet/settings/autosave_time</key>
	<applyto>/apps/stickynotes_applet/settings/autosave_time</applyto>
	<owner>stickynotes_applet</owner>
	<type>int</type>
	<default>5</default>
	<locale name="C">
		<short>Autosave timeout in minutes.</short>
		<long>
		    Every time the specified number of minutes pass. The Sticky Notes
		    are automatically saved.
		</long>
	</locale>
</schema>

<schema>
	<key>/schemas/apps/stickynotes_applet/settings/title_color</key>
	<applyto>/apps/stickynotes_applet/settings/title_color</applyto>
	<owner>stickynotes_applet</owner>
	<type>string</type>
	<default>yellow3</default>
	<locale name="C">
		<short>Background color of title</short>
		<long>
		    Background color of the title of the sticky notes. This can be any
		    string found in rgb.txt (that comes with XFree86), for example
		    "snow" or "yellow2". Or it can be an html hex specification, for example
		    "#30FF50".
		</long>
	</locale>
</schema>

<schema>
	<key>/schemas/apps/stickynotes_applet/settings/title_color_prelight</key>
	<applyto>/apps/stickynotes_applet/settings/title_color_prelight</applyto>
	<owner>stickynotes_applet</owner>
	<type>string</type>
	<default>yellow2</default>
	<locale name="C">
		<short>Background prelighted color of title</short>
		<long>
		    Background color of the title of the sticky notes when the note is in
		    focus of the keyboard and mouse. Like "body_color", this can be any
		    string found in rgb.txt (that comes with XFree86), for example
		    "snow" or "yellow2". Or it can be an html hex specification, for example
		    "#30FF50".
		</long>
	</locale>
</schema>

<schema>
	<key>/schemas/apps/stickynotes_applet/settings/body_color</key>
	<applyto>/apps/stickynotes_applet/settings/body_color</applyto>
	<owner>stickynotes_applet</owner>
	<type>string</type>
	<default>yellow2</default>
	<locale name="C">
		<short>Background color of body</short>
		<long>
		    Background color of the body of the sticky notes. This can be any
		    string found in rgb.txt (that comes with XFree86), for example
		    "snow" or "yellow2". Or it can be an html hex specification, for example
		    "#30FF50".
		</long>
	</locale>
</schema>

<schema>
	<key>/schemas/apps/stickynotes_applet/settings/body_color_prelight</key>
	<applyto>/apps/stickynotes_applet/settings/body_color_prelight</applyto>
	<owner>stickynotes_applet</owner>
	<type>string</type>
	<default>yellow1</default>
	<locale name="C">
		<short>Background prelighted color of body</short>
		<long>
		    Background color of the body of the sticky notes when the note is in
		    focus of the keyboard and mouse. Like "body_color", this can be any
		    string found in rgb.txt (that comes with XFree86), for example
		    "snow" or "yellow2". Or it can be an html hex specification, for example
		    "#30FF50".
		</long>
	</locale>
</schema>

<schema>
	<key>/schemas/apps/stickynotes_applet/settings/date_format</key>
	<applyto>/apps/stickynotes_applet/settings/date_format</applyto>
	<owner>stickynotes_applet</owner>
	<type>string</type>
	<default>%b %d, '%y</default>
	<locale name="C">
		<short>Date format</short>
		<long>
			By default, sticky notes are given the current date as the title
			when they are created. This format is used. Anything that can
			be parsed by strftime() is valid.
		</long>
	</locale>
</schema>

<schema>
	<key>/schemas/apps/stickynotes_applet/settings/sticky</key>
	<applyto>/apps/stickynotes_applet/settings/sticky</applyto>
	<owner>stickynotes_applet</owner>
	<type>bool</type>
	<default>true</default>
	<locale name="C">
		<short>Workspace stickyness</short>
		<long>
			Specifies whether the sticky notes are visible on ALL workspaces
			on the desktop, or not.
		</long>
	</locale>
</schema>

<schema>
	<key>/schemas/apps/stickynotes_applet/settings/click_behavior</key>
	<applyto>/apps/stickynotes_applet/settings/click_behavior</applyto>
	<owner>stickynotes_applet</owner>
	<type>int</type>
	<default>0</default>
	<locale name="C">
		<short>Default left-click behavior of the applet</short>
		<long>
		    Default click behavior of the applet where "0" is "New Note", and
		    "1" is "Show/Hide All Notes".
		</long>
	</locale>
</schema>

</schemalist>
</gconfschemafile>
