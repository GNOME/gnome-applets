msgstr "<guilabel>Гучномовець</guilabel>"

#: ../../../mixer/docs/C/mixer_applet2.xml:243
msgid "<guilabel>Line-in</guilabel>"
msgstr "<guilabel>Лінійний вхід</guilabel>"

#: ../../../mixer/docs/C/mixer_applet2.xml:244
msgid "<guilabel>Microphone</guilabel>"
msgstr "<guilabel>Мікрофон</guilabel>"

#: ../../../mixer/docs/C/mixer_applet2.xml:245
msgid "<guilabel>CD</guilabel>"
msgstr "<guilabel>Компакт-диск</guilabel>"

#: ../../../mixer/docs/C/mixer_applet2.xml:246
msgid "<guilabel>Record</guilabel>"
msgstr "<guilabel>Запис</guilabel>"

#: ../../../mixer/docs/C/mixer_applet2.xml:247
msgid "<guilabel>Line-1</guilabel>"
msgstr "<guilabel>Лінія-1</guilabel>"

#: ../../../mixer/docs/C/mixer_applet2.xml:248
