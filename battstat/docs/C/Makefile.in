figs = \
	menu.gif \
	general.gif \
	appear.gif \
	square.gif \
	appear-hidden.gif \
	square-hidden.gif \
	top.gif \
	bottom.gif \
        screen-ac.gif \
	screen-batt.gif \
	screen-charge.gif \
	dock.gif

