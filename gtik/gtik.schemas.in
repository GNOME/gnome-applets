<gconfschemafile>
    <schemalist>
      
      <schema>
        <key>/schemas/apps/gtik/prefs/tik_syms</key>
        <owner>gtik2_applet2</owner>
        <type>string</type>
        <default>^DJI+^IXIC+^GSPC</default>
        <locale name="C">
          <short>Stocks to monitor - must be separated by a +</short>
          <long>List containing the stocks to be displayed. Consists of the stock id separated by "+"</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/gtik/prefs/output</key>
        <owner>gtik2_applet2</owner>
        <type>bool</type>
        <default>false</default>
        <locale name="C">
          <short>Display only symbols and price</short>
          <long>Display only stock symbols along with their value. Do not display changes in value.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/gtik/prefs/scroll</key>
        <owner>gtik2_applet2</owner>
        <type>bool</type>
        <default>true</default>
        <locale name="C">
          <short>Right to left scrolling</short>
          <long>Applet scrolls right to left when this key is set to true.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/gtik/prefs/timeout</key>
        <owner>gtik2_applet2</owner>
        <type>int</type>
        <default>5</default>
        <locale name="C">
          <short>Update interval</short>
          <long>The time interval until when the applet updates the stock data.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/gtik/prefs/bgcolor</key>
        <owner>gtik2_applet2</owner>
        <type>string</type>
        <default>#ffffff</default>
        <locale name="C">
          <short>Background color</short>
          <long>The color of the background of the display. Has no effect when the user chooses to use the default theme fonts and colors.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/gtik/prefs/fgcolor</key>
        <owner>gtik2_applet2</owner>
        <type>string</type>
        <default>#000000</default>
        <locale name="C">
          <short>Unchanged color</short>
          <long>The color used when the stock's values has not changed. Has no effect when the user chooses to use the default theme fonts and colors.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/gtik/prefs/dcolor</key>
        <owner>gtik2_applet2</owner>
        <type>string</type>
        <default>#C31717</default>
        <locale name="C">
          <short>Raised color</short>
          <long>The color used when the stock's values has raised. Has no effect when the user chooses to use the default theme fonts and colors.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/gtik/prefs/ucolor</key>
        <owner>gtik2_applet2</owner>
        <type>string</type>
        <default>#188118</default>
        <locale name="C">
          <short>Lowered color</short>
          <long>The color used when the stock's values has lowered. Has no effect when the user chooses to use the default theme fonts and colors.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/gtik/prefs/use_default_theme</key>
        <owner>gtik2_applet2</owner>
        <type>bool</type>
        <default>true</default>
        <locale name="C">
          <short>Use default theme fonts and colors</short>
          <long>Use default theme fonts and colors instead of custom ones</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/gtik/prefs/font</key>
        <owner>gtik2_applet2</owner>
        <type>string</type>
        <locale name="C">
          <short>Font</short>
          <long>The font used for the display. Has no effect when the user chooses to use the default theme fonts and colors.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/gtik/prefs/font2</key>
        <owner>gtik2_applet2</owner>
        <type>string</type>
        <default>fixed 14</default>
        <locale name="C">
          <short>font 2 - No Longer used</short>
          <long></long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/gtik/prefs/buttons</key>
        <owner>gtik2_applet2</owner>
        <type>bool</type>
        <default>true</default>
        <locale name="C">
          <short>Show arrow buttons</short>
          <long>Show arrow buttons on the side of the display so that the user can scroll forward or backward.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/gtik/prefs/scroll_speed</key>
        <owner>gtik2_applet2</owner>
        <type>int</type>
        <default>40</default>
        <locale name="C">
          <short>Time in milliseconds for display update</short>
          <long>Shorter time means the stocks scroll by faster.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/gtik/prefs/width</key>
        <owner>gtik2_applet2</owner>
        <type>int</type>
        <default>200</default>
        <locale name="C">
          <short>Width of the applet</short>
          <long>The width in pixels of the applet.</long>
        </locale>
      </schema>

    </schemalist>
</gconfschemafile>
  
