#: battstat/battstat_applet.c:376 battstat/battstat_applet.c:974
msgid "Charging"
msgstr "Changement"

#: battstat/battstat_applet.c:457
#, c-format
msgid "Battery low (%d%%) and AC is offline"
msgstr ""
"La batterie est faible (%d%%) et votre ordinateur n'est pas branché sur le "
"secteur"

