#: ../battstat/battstat_applet.c:409
#, c-format
msgid "%d %s %d %s (%d%%) remaining"
msgstr "Преостанато %d %s %d %s (%d%%)"

#: ../battstat/battstat_applet.c:410 ../battstat/battstat_applet.c:417
msgid "hour"
msgid_plural "hours"
msgstr[0] "час"
msgstr[1] "часа"
msgstr[2] "часа"

