#, c-format
msgid "%d minute (%d%%) remaining"
msgid_plural "%d minutes (%d%%) remaining"
msgstr[0] "%d минута (%d%%) преостанато време"
msgstr[1] "%d минути (%d%%) преостанато време"
msgstr[2] "%d минути (%d%%) преостанато време"

