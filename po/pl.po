msgid "hour"
msgid_plural "hours"
msgstr[0] "godzina"
msgstr[1] "godziny"
msgstr[2] "godzin"

#: ../battstat/battstat_applet.c:372 ../battstat/battstat_applet.c:379
msgid "minute"
msgid_plural "minutes"
msgstr[0] "minuta"
