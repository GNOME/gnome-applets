msgstr[1] "pozostały %d godziny (%d%%)"
msgstr[2] "pozostało %d godzin (%d%%)"

#: ../battstat/battstat_applet.c:354
#, c-format
msgid "%d hour until charged (%d%%)"
msgid_plural "%d hours until charged (%d%%)"
