msgid "Battery Monitor"
msgstr "Sledování baterie"

#: ../battstat/battstat_applet.c:440 ../battstat/battstat_applet.c:500
msgid "Your battery is now fully recharged"
msgstr "Vaše baterie je nyní zcela dobita"

