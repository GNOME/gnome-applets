#: ../battstat/battstat_applet.c:401
#, c-format
msgid "%d %s %d %s (%d%%) remaining"
msgstr "Zbývá %d %s %d %s (%d%%)"

#: ../battstat/battstat_applet.c:402 ../battstat/battstat_applet.c:409
msgid "hour"
msgid_plural "hours"
msgstr[0] "hodina"
msgstr[1] "hodiny"
msgstr[2] "hodin"

#. TRANSLATOR: this is the final word in the sentence:
#. *   "Warn when time remaining is less then ?? minutes"
#.
