msgstr[1] "%d minuuttia (%d %%) jäljellä"

#: ../battstat/battstat_applet.c:352
#, c-format
msgid "%d minute until charged (%d%%)"
msgid_plural "%d minutes until charged (%d %%)"
msgstr[0] "%d minuutti kunnes akku on latautunut (%d %%)"
