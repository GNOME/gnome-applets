<oaf_info>

<oaf_server iid="OAFIID:GNOME_GWeatherApplet_Factory"
	    type="exe"
	    location="@LIBEXECDIR@/gweather-applet-2">

	<oaf_attribute name="repo_ids" type="stringv">
		<item value="IDL:Bonobo/GenericFactory:1.0"/>
		<item value="IDL:Bonobo/Unknown:1.0"/>
	</oaf_attribute>
	<oaf_attribute name="name" type="string" _value="Gweather Applet Factory"/>
	<oaf_attribute name="description" type="string" _value="Factory for creating the weather applet."/>

</oaf_server>

<oaf_server iid="OAFIID:GNOME_GWeatherApplet"
	    type="factory" 
	    location="OAFIID:GNOME_GWeatherApplet_Factory">

	<oaf_attribute name="repo_ids" type="stringv">
		<item value="IDL:GNOME/Vertigo/PanelAppletShell:1.0"/>
		<item value="IDL:Bonobo/Control:1.0"/>
		<item value="IDL:Bonobo/Unknown:1.0"/>
	</oaf_attribute>
	<oaf_attribute name="name" type="string" _value="Weather Report"/>
	<oaf_attribute name="description" type="string" _value="Monitor the current weather conditions, and forecasts"/>
	<oaf_attribute name="panel:icon" type="string" value="stock_weather-storm"/>
	<oaf_attribute name="bugzilla:bugzilla" type="string" value="GNOME"/>
        <oaf_attribute name="bugzilla:product" type="string" value="gnome-applets"/>
        <oaf_attribute name="bugzilla:component" type="string" value="gweather"/>
        <oaf_attribute name="bugzilla:other_binaries" type="string" value="gweather-applet-2"/>

</oaf_server>

</oaf_info>
