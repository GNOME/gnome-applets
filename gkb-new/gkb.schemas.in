<gconfschemafile>
    <schemalist>
        <schema>
            <key>/schemas/desktop/gnome/peripherals/keyboard/layout/num</key>
            <applyto>/desktop/gnome/peripherals/keyboard/layout/num</applyto>
            <owner>gkb_applet2</owner>
            <type>int</type>
            <default>10</default>
            <locale name="C">
                <short>Number of configured keymaps</short>
                <long>Number of configured keymaps</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/desktop/gnome/peripherals/keyboard/layout/small</key>
            <applyto>/desktop/gnome/peripherals/keyboard/layout/small</applyto>
            <owner>gkb_applet2</owner>
            <type>bool</type>
            <default>true</default>
            <locale name="C">
                <short>No longer used</short>
            </locale>
        </schema>
        <schema>
            <key>/schemas/desktop/gnome/peripherals/keyboard/layout/mode</key>
            <applyto>/desktop/gnome/peripherals/keyboard/layout/mode</applyto>
            <owner>gkb_applet2</owner>
            <type>string</type>
            <default>Label</default>
            <locale name="C">
                <short>Appearance mode of the applet</short>
                <long>It can be "Flag and Label", "Label" or "Flag"</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/desktop/gnome/peripherals/keyboard/layout/key</key>
            <applyto>/desktop/gnome/peripherals/keyboard/layout/key</applyto>
            <owner>gkb_applet2</owner>
            <type>string</type>
            <default>Mod1-Shift_L</default>
            <locale name="C">
                <short>Keybinding for switching layout</short>
                <long>This is the binding for keypress-based layout change</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/desktop/gnome/peripherals/keyboard/layout/name_0</key>
            <applyto>/desktop/gnome/peripherals/keyboard/layout/name_0</applyto>
            <owner>gkb_applet2</owner>
            <type>string</type>
            <default>US 105 key keyboard (with windows keys)</default>
            <locale name="C">
                <short>The name of the keyboard layout</short>
                <long>This will identify the layout in the list</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/desktop/gnome/peripherals/keyboard/layout/label_0</key>
            <applyto>/desktop/gnome/peripherals/keyboard/layout/label_0</applyto>
            <owner>gkb_applet2</owner>
            <type>string</type>
            <default>us</default>
            <locale name="C">
                <short>The label of the keyboard layout</short>
                <long>This will appear in the label box of the applet</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/desktop/gnome/peripherals/keyboard/layout/country_0</key>
            <applyto>/desktop/gnome/peripherals/keyboard/layout/country_0</applyto>
            <owner>gkb_applet2</owner>
            <type>string</type>
            <default>United States</default>
            <locale name="C">
                <short>The country of the keyboard layout</short>
                <long>This information helps you to identify your keyboard layout</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/desktop/gnome/peripherals/keyboard/layout/lang_0</key>
            <applyto>/desktop/gnome/peripherals/keyboard/layout/lang_0</applyto>
            <owner>gkb_applet2</owner>
            <type>string</type>
            <default>English</default>
            <locale name="C">
                <short>The language of the keyboard layout</short>
                <long>This information helps you to identify your keyboard layout</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/desktop/gnome/peripherals/keyboard/layout/flag_0</key>
            <applyto>/desktop/gnome/peripherals/keyboard/layout/flag_0</applyto>
            <owner>gkb_applet2</owner>
            <type>string</type>
            <default>us.png</default>
            <locale name="C">
                <short>Flag image for the keyboard layout</short>
                <long>This flag will appear in flag field of the applet</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/desktop/gnome/peripherals/keyboard/layout/command_0</key>
            <applyto>/desktop/gnome/peripherals/keyboard/layout/command_0</applyto>
            <owner>gkb_applet2</owner>
            <type>string</type>
            <default>gkb_xmmap us</default>
            <locale name="C">
                <short>The command that switches your keymap to this layout</short>
                <long>This should be like "gkb_xmmap us", "setxkbmap us" or "xmodmap something.xmm"</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/desktop/gnome/peripherals/keyboard/layout/name_1</key>
            <applyto>/desktop/gnome/peripherals/keyboard/layout/name_1</applyto>
            <owner>gkb_applet2</owner>
            <type>string</type>
            <default>Hungarian 105 keys latin2</default>
            <locale name="C">
                <short>The name of the keyboard layout</short>
                <long>This will identify the layout in the list</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/desktop/gnome/peripherals/keyboard/layout/label_1</key>
            <applyto>/desktop/gnome/peripherals/keyboard/layout/label_1</applyto>
            <owner>gkb_applet2</owner>
            <type>string</type>
            <default>hu</default>
            <locale name="C">
                <short>The label of the keyboard layout</short>
                <long>This will appear in the label box of the applet</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/desktop/gnome/peripherals/keyboard/layout/country_1</key>
            <applyto>/desktop/gnome/peripherals/keyboard/layout/country_1</applyto>
            <owner>gkb_applet2</owner>
            <type>string</type>
            <default>Hungary</default>
            <locale name="C">
                <short>The country of the keyboard layout</short>
                <long>This information helps you to identify your keyboard layout</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/desktop/gnome/peripherals/keyboard/layout/lang_1</key>
            <applyto>/desktop/gnome/peripherals/keyboard/layout/lang_1</applyto>
            <owner>gkb_applet2</owner>
            <type>string</type>
            <default>Hungarian</default>
            <locale name="C">
                <short>The language of the keyboard layout</short>
                <long>This information helps you to identify your keyboard layout</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/desktop/gnome/peripherals/keyboard/layout/flag_1</key>
            <applyto>/desktop/gnome/peripherals/keyboard/layout/flag_1</applyto>
            <owner>gkb_applet2</owner>
            <type>string</type>
            <default>hu.png</default>
            <locale name="C">
                <short>Flag image for the keyboard layout</short>
                <long>This flag will appear in flag field of the applet</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/desktop/gnome/peripherals/keyboard/layout/command_1</key>
            <applyto>/desktop/gnome/peripherals/keyboard/layout/command_1</applyto>
            <owner>gkb_applet2</owner>
            <type>string</type>
            <default>gkb_xmmap hu</default>
            <locale name="C">
                <short>The command that switches your keymap to this layout</short>
                <long>This should be like "gkb_xmmap us", "setxkbmap us" or "xmodmap something.xmm"</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/desktop/gnome/peripherals/keyboard/layout/current</key>
	    <applyto>/desktop/gnome/peripherals/keyboard/layout/current</applyto>
            <owner>gkb_applet2</owner>
            <type>int</type>
            <default>0</default>
            <locale name="C">
                <short>The default keymap</short>
                <long>The default keymap for the applet</long>
            </locale>
        </schema>
    </schemalist>
</gconfschemafile>
