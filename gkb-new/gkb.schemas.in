<gconfschemafile>
    <schemalist>
        <schema>
            <key>/schemas/apps/gkb-applet/prefs/num</key>
            <owner>gkb-applet</owner>
            <type>int</type>
            <default>10</default>
            <locale name="C">
                <short>Number of configured keymaps</short>
                <long>Number of configured keymaps</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/apps/gkb-applet/prefs/small</key>
            <owner>gkb-applet</owner>
            <type>bool</type>
            <default>true</default>
            <locale name="C">
                <short>No longer used</short>
            </locale>
        </schema>
        <schema>
            <key>/schemas/apps/gkb-applet/prefs/mode</key>
            <owner>gkb-applet</owner>
            <type>string</type>
            <default>Flag and Label</default>
            <locale name="C">
                <short>Appearance mode of the applet</short>
                <long>It can be "Flag and Label", "Label" or "Flag"</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/apps/gkb-applet/prefs/key</key>
            <owner>gkb-applet</owner>
            <type>string</type>
            <default>Mod1-Shift_L</default>
            <locale name="C">
                <short>Keybinding for switching layout</short>
                <long>This is the binding for keypress-based layout change</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/apps/gkb-applet/prefs/name_0</key>
            <owner>gkb-applet</owner>
            <type>string</type>
            <default>US 105 key keyboard (with windows keys)</default>
            <locale name="C">
                <short>The name of the keyboard layout</short>
                <long>This will identify the layout in the list</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/apps/gkb-applet/prefs/label_0</key>
            <owner>gkb-applet</owner>
            <type>string</type>
            <default>us</default>
            <locale name="C">
                <short>The label of the keyboard layout</short>
                <long>This will appear in the label box of the applet</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/apps/gkb-applet/prefs/country_0</key>
            <owner>gkb-applet</owner>
            <type>string</type>
            <default>United States</default>
            <locale name="C">
                <short>The country of the keyboard layout</short>
                <long>This information helps you to identify your keyboard layout</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/apps/gkb-applet/prefs/lang_0</key>
            <owner>gkb-applet</owner>
            <type>string</type>
            <default>English</default>
            <locale name="C">
                <short>The language of the keyboard layout</short>
                <long>This information helps you to identify your keyboard layout</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/apps/gkb-applet/prefs/flag_0</key>
            <owner>gkb-applet</owner>
            <type>string</type>
            <default>us.png</default>
            <locale name="C">
                <short>Flag image for the keyboard layout</short>
                <long>This flag will appear in flag field of the applet</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/apps/gkb-applet/prefs/command_0</key>
            <owner>gkb-applet</owner>
            <type>string</type>
            <default>gkb_xmmap us</default>
            <locale name="C">
                <short>The command that switches your keymap to this layout</short>
                <long>This should be like "gkb_xmmap us", "setxkbmap us" or "xmodmap something.xmm"</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/apps/gkb-applet/prefs/name_1</key>
            <owner>gkb-applet</owner>
            <type>string</type>
            <default>Hungarian 105 keys latin2</default>
            <locale name="C">
                <short>The name of the keyboard layout</short>
                <long>This will identify the layout in the list</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/apps/gkb-applet/prefs/label_1</key>
            <owner>gkb-applet</owner>
            <type>string</type>
            <default>hu</default>
            <locale name="C">
                <short>The label of the keyboard layout</short>
                <long>This will appear in the label box of the applet</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/apps/gkb-applet/prefs/country_1</key>
            <owner>gkb-applet</owner>
            <type>string</type>
            <default>Hungary</default>
            <locale name="C">
                <short>The country of the keyboard layout</short>
                <long>This information helps you to identify your keyboard layout</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/apps/gkb-applet/prefs/lang_1</key>
            <owner>gkb-applet</owner>
            <type>string</type>
            <default>Hungarian</default>
            <locale name="C">
                <short>The language of the keyboard layout</short>
                <long>This information helps you to identify your keyboard layout</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/apps/gkb-applet/prefs/flag_1</key>
            <owner>gkb-applet</owner>
            <type>string</type>
            <default>hu.png</default>
            <locale name="C">
                <short>Flag image for the keyboard layout</short>
                <long>This flag will appear in flag field of the applet</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/apps/gkb-applet/prefs/command_1</key>
            <owner>gkb-applet</owner>
            <type>string</type>
            <default>gkb_xmmap hu</default>
            <locale name="C">
                <short>The command that switches your keymap to this layout</short>
                <long>This should be like "gkb_xmmap us", "setxkbmap us" or "xmodmap something.xmm"</long>
            </locale>
        </schema>
    </schemalist>
</gconfschemafile>
